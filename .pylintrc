[REPORTS]
# Use colors rather than plain text.
output-format=colorized
# Only display the messages, not the full report.
reports=no
# Display the ID of the errors and warnings to make them easier to disable.
include-ids=y

[MESSAGES CONTROL]
# We don't want to see suppressed warnings, TODOs, classes with few public
# members (like ndb.Model subclasses), classes with too many public members
# (like tests), or modules with no member (since pylint doesn't deal with the
# dynamic imports in ndb properties correctly).
disable=I0011,W0511,E1101,R0903,R0904

[BASIC]
# Default good names plus setUp and tearDown for unit tests.
good-names=setUp,tearDown,i,j,k,ex,Run,_
